	month = jan,
	year = {2015},
	pages = {170--177},
	file = {ScienceDirect Full Text PDF:/home/tomas/Zotero/storage/TSPRRK5Y/Ivanov et al. - 2015 - Analysis of Intron Sequences Reveals Hallmarks of .pdf:application/pdf;ScienceDirect Snapshot:/home/tomas/Zotero/storage/2KQNYE3E/S2211124714010481.html:text/html}
}


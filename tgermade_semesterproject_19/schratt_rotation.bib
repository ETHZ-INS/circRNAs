@article{agarwal_predicting_2015,
	title = {Predicting effective {microRNA} target sites in mammalian {mRNAs}},
	volume = {4},
	issn = {2050-084X},
	url = {https://doi.org/10.7554/eLife.05005},
	doi = {10.7554/eLife.05005},
	abstract = {MicroRNA targets are often recognized through pairing between the miRNA seed region and complementary sites within target mRNAs, but not all of these canonical sites are equally effective, and both computational and in vivo UV-crosslinking approaches suggest that many mRNAs are targeted through non-canonical interactions. Here, we show that recently reported non-canonical sites do not mediate repression despite binding the miRNA, which indicates that the vast majority of functional sites are canonical. Accordingly, we developed an improved quantitative model of canonical targeting, using a compendium of experimental datasets that we pre-processed to minimize confounding biases. This model, which considers site type and another 14 features to predict the most effectively targeted mRNAs, performed significantly better than existing models and was as informative as the best high-throughput in vivo crosslinking approaches. It drives the latest version of TargetScan (v7.0; targetscan.org), thereby providing a valuable resource for placing miRNAs into gene-regulatory networks.},
	urldate = {2019-05-25},
	journal = {eLife},
	author = {Agarwal, Vikram and Bell, George W and Nam, Jin-Wu and Bartel, David P},
	editor = {Izaurralde, Elisa},
	month = aug,
	year = {2015},
	keywords = {miRNA target, miRNA target prediction, miRNA-binding site, post-transcriptional gene regulation},
	pages = {e05005},
	file = {Full Text PDF:/home/tomas/Zotero/storage/WNBVEV7G/Agarwal et al. - 2015 - Predicting effective microRNA target sites in mamm.pdf:application/pdf}
}

@article{li_biogenesis_2018,
	title = {The {Biogenesis}, {Functions}, and {Challenges} of {Circular} {RNAs}},
	volume = {71},
	issn = {1097-2765},
	url = {http//www.sciencedirect.com/science/article/pii/S1097276518305094},
	doi = {10.1016/j.molcel.2018.06.034},
	abstract = {Covalently closed circular RNAs (circRNAs) are produced by precursor mRNA back-splicing of exons of thousands of genes in eukaryotes. circRNAs are generally expressed at low levels and often exhibit cell-type-specific and tissue-specific patterns. Recent studies have shown that their biogenesis requires spliceosomal machinery and can be modulated by both cis complementary sequences and protein factors. The functions of most circRNAs remain largely unexplored, but known functions include sequestration of microRNAs or proteins, modulation of transcription and interference with splicing, and even translation to produce polypeptides. However, challenges exist at multiple levels to understanding of the regulation of circRNAs because of their circular conformation and sequence overlap with linear mRNA counterparts. InÂ this review, we survey the recent progress on circRNA biogenesis and function and discuss technical obstacles in circRNA studies.},
	number = {3},
	urldate = {2019-05-25},
	journal = {Molecular Cell},
	author = {Li, Xiang and Yang, Li and Chen, Ling-Ling},
	month = aug,
	year = {2018},
	keywords = {back-splicing, biogenesis, circRNA, circular RNA, function, intronic complementary sequences, linear RNA, RNA binding proteins, RNA-seq, spliceosomal machinery},
	pages = {428--442},
	file = {ScienceDirect Full Text PDF:/home/tomas/Zotero/storage/3KQNFAWQ/Li et al. - 2018 - The Biogenesis, Functions, and Challenges of Circu.pdf:application/pdf;ScienceDirect Snapshot:/home/tomas/Zotero/storage/63ILCPN2/S1097276518305094.html:text/html}
